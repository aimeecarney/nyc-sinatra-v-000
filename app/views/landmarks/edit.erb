<h1>Edit a Landmark</h1>

<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input type="hidden" name="_method" value="PATCH">
 <label>Please Enter the Landmark Name:</label>
  <br></br>
  <input type="text" name="landmark[name]" id="name" value=<%=@landmark.name%> >
  <br></br>
  <label>Please Enter a Year Completed:</label>
  <br></br>
  <input id="year_completed" type="text" name="landmark[year_completed]" value=<%=@landmark.year_completed%>>
  <br></br>
  <input type="submit" value="Edit Landmark">
  </form>
